<!--pages/locate/locate.wxml-->
<view class="container">
  <map
      id="myMap"
      style="width: 100%; height: 400rpx;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      show-location
    >
    <cover-view class="controls"> <cover-view class="icon iconfont icon-md-locate"></cover-view></cover-view>
    </map>
</view>
